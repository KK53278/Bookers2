<div class="container px-5 px-sm-0">
<div class="row">
  <%= render 'books/list', user: @user,book: @book %>

  <div class="col-md-8 offset-md-1">
  <h2>Users</h2>
    <table class="table table-hover table-inverse">

  <thead>
      <th>image</th>
      <th>name</th>
  </thead>

    <tbody>
    <% @users.each do |f| %>
      <tr>
      <td>
        <%= link_to user_path(f.id) do %>
        <%= image_tag f.get_profile_image(60,60) %>
        <% end %>
      </td>

      <td>
        <%= f.name %>
      </td>
      <td>
        <%=link_to "Show", user_path(f.id) %>
      </td>

        </tr>
    <%end%>
    </tbody>
    </table>
  </div>

</div>
</div>